{
  "name": "sonken",
  "version": "1.0.0",
  "description": "This product adopts Google's Material Design for UI.",
  "author": "Mamoru Hashimoto <you@example.com>",
  "scripts": {
    "start:ssr":
       "supervisor -q -n error -w dist -i node_modules dist/ssr.node.js",
    "start:api":
       "supervisor -q -n error -w dist -i node_modules dist/api.node.js",
    "start:mms": "node -r ./src/hook/babel-register.js src/mms-server.js",
    "start:dev": "node -r ./src/hook/babel-register.js src/dev-server.js",
    "start:node": "npm-run-all -p start:api start:ssr watch:node",
    "build:node": "webpack --config webpack.node.js",
    "watch:node": "webpack --config webpack.node.js -w --colors",
    "start:web": "npm-run-all -p start:mms start:dev",
    "build:web": "webpack --config webpack.production.js",
    "watch:web": "webpack --config webpack.development.js",
    "start:all": "npm-run-all -p start:api start:ssr start:mms start:dev",
    "build:all": "npm-run-all -p build:node build:web",
    "watch:all": "npm-run-all -p watch:node watch:web",
    "release": "docker/install.sh"
  },
  "license": "ISC",
  "dependencies": {
    "body-parser": "1.18.2",
    "connect-mongo": "2.0.1",
    "dotenv": "4.0.0",
    "express": "4.16.2",
    "express-session": "1.15.6",
    "flux": "3.1.3",
    "log4js": "2.5.2",
    "marked": "0.3.12",
    "material-ui": "1.0.0-beta.35",
    "material-ui-icons": "1.0.0-beta.17",
    "mongoose": "5.0.8",
    "node-libs-browser": "^2.1.0",
    "prop-types": "15.6.0",
    "ramda": "0.25.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-hot-loader": "3.1.3",
    "react-jss": "8.2.1",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.2.2",
    "rx": "4.1.0",
    "serve-favicon": "2.4.5",
    "serve-static": "1.13.1",
    "xml2js": "0.4.19",
    "xmlbuilder": "9.0.4",
    "ya-csv": "0.10.1"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-es3-member-expression-literals": "6.22.0",
    "babel-plugin-transform-es3-property-literals": "6.22.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "babel-register": "6.26.0",
    "clean-webpack-plugin": "0.1.17",
    "dotenv-webpack": "1.5.4",
    "eslint": "4.16.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-react": "7.6.0",
    "file-loader": "1.1.6",
    "http-proxy-middleware": "0.17.4",
    "npm-run-all": "4.1.2",
    "smtp-server": "3.4.1",
    "supervisor": "0.12.0",
    "uglifyjs-webpack-plugin": "1.1.6",
    "webpack": "3.10.0",
    "webpack-dev-middleware": "2.0.4",
    "webpack-dev-server": "2.11.1",
    "webpack-hot-middleware": "2.21.0",
    "webpack-manifest-plugin": "1.3.2",
    "webpack-merge": "4.1.1",
    "webpack-node-externals": "1.6.0"
  }
}
