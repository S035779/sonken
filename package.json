{
  "name": "sonken",
  "version": "1.0.0",
  "description": "This product adopts Google's Material Design for UI.",
  "author": "Mamoru Hashimoto <you@example.com>",
  "scripts": {
    "start:all":  "npm-run-all -p start:job start:api start:ssr start:mms start:dev",
    "start:web":  "npm-run-all -p start:mms start:dev",
    "start:ssr":  "supervisor -q -n error -w dist -i node_modules dist/ssr.node.js",
    "start:api":  "supervisor -q -n error -w dist -i node_modules dist/api.node.js",
    "start:job":  "supervisor -q -n error -w dist -i node_modules dist/job.node.js",
    "start:mms":  "node -r ./src/hook/babel-register.js src/mms-server.js",
    "start:dev":  "node -r ./src/hook/babel-register.js src/dev-server.js",
    "start:node": "cross-env API_URL=http://localhost:8080/api npm-run-all -p start:job start:api start:ssr watch:node",
    "build:all":  "npm-run-all -p build:node build:web",
    "build:web":  "cross-env NODE_ENV=production webpack --config webpack.production.js",
    "build:node": "cross-env NODE_ENV=production webpack --config webpack.node.js",
    "stage:all":  "npm-run-all -p stage:node stage:web",
    "stage:web":  "cross-env NODE_ENV=staging webpack --config webpack.staging.js",
    "stage:node": "cross-env NODE_ENV=staging webpack --config webpack.node.js",
    "watch:all":  "npm-run-all -p watch:node watch:web",
    "watch:web":  "webpack --config webpack.development.js",
    "watch:node": "webpack --config webpack.node.js -w --colors",
    "release":    "docker/install.sh"
  },
  "license": "ISC",
  "dependencies": {
    "async": "2.6.0",
    "async-es": "2.6.0",
    "body-parser": "1.18.2",
    "classnames": "2.2.5",
    "connect-mongo": "2.0.1",
    "dotenv": "4.0.0",
    "draft-js": "0.10.5",
    "encoding-japanese": "1.0.29",
    "express": "4.16.2",
    "express-session": "1.15.6",
    "flux": "3.1.3",
    "iconv": "2.3.0",
    "log4js": "2.5.2",
    "marked": "0.3.12",
    "material-ui": "1.0.0-beta.47",
    "material-ui-icons": "1.0.0-beta.17",
    "mongoose": "5.0.8",
    "node-libs-browser": "2.1.0",
    "osmosis": "1.1.6",
    "prop-types": "15.6.0",
    "ramda": "0.25.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-hot-loader": "3.1.3",
    "react-jss": "8.2.1",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.2.2",
    "rxjs": "5.5.8",
    "serve-favicon": "2.4.5",
    "serve-static": "1.13.1",
    "xml2js": "0.4.19"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-es3-member-expression-literals": "6.22.0",
    "babel-plugin-transform-es3-property-literals": "6.22.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "babel-register": "6.26.0",
    "clean-webpack-plugin": "0.1.17",
    "cross-env": "5.1.4",
    "dotenv-webpack": "1.5.4",
    "eslint": "4.16.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-react": "7.6.0",
    "file-loader": "1.1.6",
    "http-proxy-middleware": "0.17.4",
    "npm-run-all": "4.1.2",
    "smtp-server": "3.4.1",
    "supervisor": "0.12.0",
    "uglifyjs-webpack-plugin": "1.1.6",
    "webpack": "3.10.0",
    "webpack-dev-middleware": "2.0.4",
    "webpack-dev-server": "2.11.1",
    "webpack-hot-middleware": "2.21.0",
    "webpack-manifest-plugin": "1.3.2",
    "webpack-merge": "4.1.1",
    "webpack-node-externals": "1.6.0"
  }
}
