{
  "name": "sonken",
  "version": "1.0.0",
  "private": true,
  "description": "This product adopts Google's Material Design for UI.",
  "author": "Mamoru Hashimoto <you@example.com>",
  "scripts": {
    "start:all": "npm-run-all -p start:job start:api start:ssr start:mms start:dev",
    "start:web": "npm-run-all -p start:mms start:dev",
    "start:mms": "node -r ./src/hook/babel-register src/mms-server.js",
    "start:dev": "cross-env NODE_ENV=development node -r ./src/hook/babel-register src/dev-server.js",
    "start:node": "npm-run-all -p start:api start:ssr watch:node",
    "start:api": "supervisor -q -n error -w dist -i node_modules dist/api.node.js",
    "start:ssr": "supervisor -q -n error -w dist -i node_modules dist/ssr.node.js",
    "dump:api": "node --expose-gc dist/api.node.js",
    "dump:ssr": "node --expose-gc dist/ssr.node.js",
    "prof:api": "node --prof dist/api.node.js",
    "prof:ssr": "node --prof dist/ssr.node.js",
    "prof-process:api": "node --prof-process isolate-0xNNNNNNNNNNNNN-v8.log",
    "prof-process:ssr": "node --prof-process isolate-0xNNNNNNNNNNNNN-v8.log",
    "graph:api": "0x dist/api.node.js",
    "graph:ssr": "0x dist/ssr.node.js",
    "watch:node": "cross-env NODE_ENV=development webpack --config webpack.node.js --watch --colors",
    "start:job": "cross-env WORKER_NAME=job-worker supervisor -q -n error -w dist -i node_modules dist/job.node.js",
    "start:itm": "cross-env WORKER_NAME=itm-worker supervisor -q -n error -w dist -i node_modules dist/itm.node.js",
    "start:img": "cross-env WORKER_NAME=img-worker supervisor -q -n error -w dist -i node_modules dist/img.node.js",
    "start:dfg": "cross-env WORKER_NAME=dfg-worker supervisor -q -n error -w dist -i node_modules dist/dfg.node.js",
    "start:att": "cross-env WORKER_NAME=att-worker supervisor -q -n error -w dist -i node_modules dist/att.node.js",
    "start:its": "cross-env WORKER_NAME=its-worker supervisor -q -n error -w dist -i node_modules dist/its.node.js",
    "start:arc": "cross-env WORKER_NAME=arc-worker supervisor -q -n error -w dist -i node_modules dist/arc.node.js",
    "start:wks": "cross-env WORKER_NAME=wks-worker supervisor -q -n error -w dist -i node_modules dist/wks.node.js",
    "build:production": "npm-run-all -p build:pro:node build:pro:web",
    "build:staging": "npm-run-all -p build:stg:node build:stg:web",
    "build:development": "npm-run-all -p build:dev:node build:dev:web",
    "build:pro:web": "cross-env NODE_ENV=production  webpack --config webpack.production.js",
    "build:stg:web": "cross-env NODE_ENV=staging     webpack --config webpack.staging.js",
    "build:dev:web": "cross-env NODE_ENV=development webpack --config webpack.development.js",
    "build:pro:node": "cross-env NODE_ENV=production  webpack --config webpack.node.js",
    "build:stg:node": "cross-env NODE_ENV=staging     webpack --config webpack.node.js",
    "build:dev:node": "cross-env NODE_ENV=development webpack --config webpack.node.js",
    "release": "/bin/bash systemd/install.sh",
    "restart": "/bin/bash systemd/restart.sh"
  },
  "license": "ISC",
  "dependencies": {
    "@loadable/component": "^5.2.2",
    "@loadable/server": "^5.2.2",
    "@material-ui/core": "3.8.3",
    "@material-ui/icons": "3.0.2",
    "accepts": "^1.3.5",
    "agenda": "^2.0.2",
    "archiver": "^3.0.0",
    "async": "^2.6.1",
    "aws-sdk": "2.387.0",
    "body-parser": "1.18.3",
    "bytes": "^3.0.0",
    "classnames": "2.2.6",
    "compressible": "^2.0.15",
    "connect-mongo": "2.0.3",
    "cookie-parser": "^1.4.3",
    "dotenv": "6.2.0",
    "encoding-japanese": "1.0.29",
    "express": "4.16.4",
    "express-session": "1.15.6",
    "flux": "3.1.3",
    "fs-extra": "^7.0.1",
    "heapdump": "^0.3.12",
    "iconv": "2.3.2",
    "iltorb": "^2.4.1",
    "log4js": "3.0.6",
    "lzma-native": "^4.0.3",
    "marked": "0.6.0",
    "mongoose": "5.4.3",
    "nodemailer": "5.1.1",
    "nodemailer-smtp-transport": "2.7.4",
    "osmosis": "^1.1.8",
    "promise-throttle": "^1.0.0",
    "prop-types": "15.6.2",
    "ramda": "0.26.1",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-hot-loader": "4.6.3",
    "react-jss": "^8.6.1",
    "react-router": "4.3.1",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.3.1",
    "rxjs": "6.3.3",
    "rxjs-compat": "^6.2.2",
    "serve-favicon": "2.5.0",
    "serve-static": "1.13.2",
    "source-map-support": "^0.5.6",
    "stream-concat": "^0.3.0",
    "vary": "^1.1.2",
    "xml2js": "0.4.19",
    "yazl": "^2.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-member-expression-literals": "^7.0.0",
    "@babel/plugin-transform-property-literals": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-0": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@loadable/babel-plugin": "^5.2.2",
    "@loadable/webpack-plugin": "^5.2.2",
    "babel-eslint": "10.0.1",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "1.0.0",
    "cross-env": "5.2.0",
    "css-loader": "2.1.0",
    "dotenv-webpack": "^1.5.7",
    "eslint": "^5.4.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "3.0.1",
    "http-proxy-middleware": "0.19.1",
    "mini-css-extract-plugin": "0.5.0",
    "npm-run-all": "4.1.5",
    "smtp-server": "3.5.0",
    "style-loader": "^0.23.1",
    "supervisor": "0.12.0",
    "uglifyjs-webpack-plugin": "2.1.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-hot-middleware": "^2.22.3",
    "webpack-manifest-plugin": "2.0.4",
    "webpack-merge": "4.2.1",
    "webpack-node-externals": "1.7.2"
  }
}
