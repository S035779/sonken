{
  "name": "sonken",
  "version": "1.0.0",
  "description": "This product adopts Google's Material Design for UI.",
  "author": "Mamoru Hashimoto <you@example.com>",
  "scripts": {
    "start:all": "npm-run-all -p start:job start:api start:ssr start:mms start:dev",
    "start:web": "npm-run-all -p start:mms start:dev",
    "start:ssr": "supervisor -q -n error -w dist -i node_modules dist/ssr.node.js",
    "start:api": "supervisor -q -n error -w dist -i node_modules dist/api.node.js",
    "start:job": "supervisor -q -n error -w dist -i node_modules dist/job.node.js",
    "start:img": "supervisor -q -n error -w dist -i node_modules dist/img.node.js",
    "start:mms": "node -r ./src/hook/babel-register src/mms-server.js",
    "start:dev": "node -r ./src/hook/babel-register src/dev-server.js",
    "start:node": "cross-env API_URL=http://localhost:8080/api APP_NAME=\"RSS Reader!!\" npm-run-all -p start:job start:api start:ssr watch:node",
    "build:all": "npm-run-all -p build:node build:web",
    "build:staging": "npm-run-all -p build:stg:node build:stg:web",
    "build:development": "npm-run-all -p build:dev:node build:dev:web",
    "build:web": "webpack --config webpack.production.js",
    "build:stg:web": "webpack --config webpack.staging.js",
    "build:dev:web": "webpack --config webpack.development.js",
    "build:node": "cross-env NODE_ENV=production webpack --config webpack.node.js",
    "build:stg:node": "cross-env NODE_ENV=staging webpack --config webpack.node.js",
    "build:dev:node": "cross-env NODE_ENV=development webpack --config webpack.node.js",
    "watch:node": "webpack --config webpack.node.js --watch --colors",
    "release:staging": "/bin/bash systemd/install.sh",
    "restart:staging": "/bin/bash systemd/restart.sh"
  },
  "license": "ISC",
  "dependencies": {
    "@material-ui/core": "^1.4.3",
    "@material-ui/icons": "^2.0.1",
    "async": "^2.6.1",
    "aws-sdk": "2.282.1",
    "body-parser": "1.18.2",
    "classnames": "2.2.5",
    "connect-mongo": "2.0.1",
    "dotenv": "6.0.0",
    "encoding-japanese": "1.0.29",
    "express": "4.16.2",
    "express-session": "1.15.6",
    "flux": "3.1.3",
    "iconv": "2.3.0",
    "log4js": "3.0.2",
    "marked": "0.3.12",
    "mongoose": "^5.2.6",
    "nodemailer": "4.6.7",
    "nodemailer-smtp-transport": "2.7.4",
    "osmosis": "^1.1.8",
    "promise-throttle": "^1.0.0",
    "prop-types": "15.6.0",
    "ramda": "0.25.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^4.3.4",
    "react-jss": "^8.6.1",
    "react-router": "^4.3.1",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.3.1",
    "rxjs": "^6.2.2",
    "rxjs-compat": "^6.2.2",
    "serve-favicon": "2.4.5",
    "serve-static": "1.13.1",
    "source-map-support": "^0.5.6",
    "xml2js": "0.4.19"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-loader": "7.1.5",
    "babel-plugin-transform-es3-member-expression-literals": "6.22.0",
    "babel-plugin-transform-es3-property-literals": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "cache-loader": "^1.2.2",
    "clean-webpack-plugin": "0.1.19",
    "cross-env": "5.1.4",
    "eslint": "4.16.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-react": "7.6.0",
    "file-loader": "1.1.11",
    "http-proxy-middleware": "0.18.0",
    "npm-run-all": "4.1.3",
    "smtp-server": "3.4.1",
    "supervisor": "0.12.0",
    "uglifyjs-webpack-plugin": "1.2.7",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-hot-middleware": "^2.22.3",
    "webpack-manifest-plugin": "2.0.3",
    "webpack-merge": "4.1.3",
    "webpack-node-externals": "1.6.0"
  }
}
